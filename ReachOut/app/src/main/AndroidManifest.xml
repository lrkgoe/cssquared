<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.reachout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomePageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MakeEventActivity" />
        <activity android:name=".ListOfEventsActivity" />
        <activity android:name=".EventDetailsActivity" />
        <activity android:name=".StartedEventsActivity" />
        <activity android:name=".SignedUpEventsActivity" />
        <activity android:name=".UserProfileActivity"></activity>
    </application>

</manifest>
